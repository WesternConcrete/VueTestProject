    <form>\n            <div class=\"row\">\n            <div class=\"cell\">\n                <label>Type:</label>\n            </div>\n            <div class=\"cell\">\n                <em>{{ product.type }}</em>\n            </div>\n            </div>\n            <div class=\"row\">\n            <div class=\"cell\">\n                <label>Price:</label>\n            </div>\n            <div class=\"cell\">\n                ${{ product.price.USD.toFixed(2) }}\n            </div>\n            </div>\n            <div class=\"row\">\n            <div class=\"cell\">\n                <label>Quantity:</label>\n            </div>\n            <div class=\"cell\">\n                <input type=\"number\" v-model.number=\"quantity\">\n            </div>\n            </div>\n        </form>\n        </div>\n        <div class=\"card-footer\">\n        <button @click=\"addToCart(product.name, quantity)\" class=\"btn btn-light\">\n            Add to cart\n        </button>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  props: ['product', 'addToCart'],\n  data () {\n    return {\n      quantity: 0\n    }\n  }\n}\n</script>\n"]}]}