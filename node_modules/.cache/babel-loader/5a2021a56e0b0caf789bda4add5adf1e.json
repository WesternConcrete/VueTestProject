9B,KAJ4C,EAK5C,mBAAA,CAAqD,IAArD,EAAqD,IAArD,EAAI,MAAC,gBAAA,CAAA,CAAI,QAAQ,GAAG,QAAA,CAAA,QAAA,CAAS,GAAT,CAAf,EAA8B,OAA9B,CAAqC,CAArC,CAAA,CAAL,EAA0C;AAAA;AAA1C,KAL4C,EAM5C,mBAAA,CAIK,IAJL,EAAA,WAAA,EAIK,CAHH,mBAAA,CAES,QAFT,EAES;AAFA,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,MAAA,CAAA,MAAA,CAAO,GAAP,CAAF;AAAA,OAEL;AAFoB,MAAA,KAAK,EAAC;AAE1B,KAFT,EAA+D,KAA/D,EAEA;AAAA;AAFA,MAEA,WAFA,CAGG,CAJL,CAN4C,CAA9C,C;GAWK,CAXL,E;;AAAA,GAYM,EAbR,CAcM,CAzBR,CAgCI,E,CALM,MAAM,CAAC,IAAP,CAAY,MAAA,CAAA,IAAZ,EAAkB,M,kBAA5B,mBAAA,CAAgF,GAAhF,EAAA,WAAA,EAAgF,WAAhF,C,qCAKI,EAJJ,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFJ,mBAAA,CAA4D,MAA5D,EAA4D,IAA5D,EAA4D,CAAtD,WAAsD,E,iBAA/B,OAAE,gBAAA,CAAG,QAAA,CAAA,cAAA,EAAH,C,EAAiB;AAAA;GAAY,CAA5D,CAEI,EADJ,WACI,CAHN,CAII,CAjCN,CAkCI,CA3CN,CA4CM,CA7CN,C","sourcesContent":["<template>\n    <aside class=\"cart-container\">\n    <div class=\"cart\">\n      <h1 class=\"cart-title spread\">\n        <span>\n          Cart\n          <i class=\"icofont-cart-alt icofont-1x\"></i>\n        </span>\n        <button @click=\"toggle\" class=\"cart-close\">&times;</button>\n      </h1>\n\n      <div class=\"cart-body\">\n        <table class=\"cart-table\">\n          <thead>\n            <tr>\n              <th><span class=\"sr-only\">Product Image</span></th>\n              <th>Product</th>\n              <th>Price</th>\n              <th>Qty</th>\n              <th>Total</th>\n              <th><span class=\"sr-only\">Actions</span></th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(quantity, key, i) in cart\" :key=\"i\">\n              <td><i class=\"icofont-carrot icofont-3x\"></i></td>\n              <td>{{ getPrice(key) }}</td>\n              <td>${{ getPrice(key) }}</td>\n              <td class=\"center\">{{ quantity }}</td>\n              <td>${{ (quantity * getPrice(key)).toFixed(2) }}</td>\n              <td class=\"center\">\n                <button @click=\"remove(key)\" class=\"btn btn-light cart-remove\">\n                  &times;\n                </button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n\n        <p v-if=\"!Object.keys(cart).length\" class=\"center\"><em>No items in cart</em></p>\n        <div class=\"spread\">\n          <span><strong>Total:</strong> ${{ calculateTotal() }}</span>\n          <button class=\"btn btn-light\">Checkout</button>\n        </div>\n      </div>\n    </div>\n  </aside>\n</template>\n\n<script>\n\nexport default {\n  props: ['toggle', 'cart', 'inventory', 'remove'],\n  methods: {\n    getPrice (name) {\n      const product = this.inventory.find((p) => {\n        return p.name === name\n      })\n      return product.price.USD.toFixed(2)\n    },\n    calculateTotal () {\n      const total = Object.entries(this.cart).reduce((acc, curr, index) => {\n        return acc + (curr[1] * this.getPrice(curr[0]))\n      }, 0)\n      return total.toFixed(2)\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}